    "install": [
        "esy-installer oniguruma.install"
    ]
  },
  "resolutions": {
    "@opam/biniou": "1.2.0",
    "@opam/easy-format": "1.3.1"
  },
  "dependencies": {
    "@opam/dune": "^1.6.0",
    "@esy-ocaml/reason": "^3.4.0",
    "@reason-native/rely": "^1.3.1",
    "refmterr": "*",
    "ocaml": ">=4.6.0",
    "esy-oniguruma": "^5.9.1001",
    "reperf": "^1.3.0"
  },
  "devDependencies": {
    "ocaml": ">=4.6.0"
  }
}
